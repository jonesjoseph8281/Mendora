

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Business {
  id          String   @id @default(uuid())
  name        String
  category    String
  location    String?
  description String?
  businessEmail String?
  contact     String?
  imageUrl     String?

}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  
  
}
